        "@types/aos": "3.0.3",
        "@types/chart.js": "2.9.28",
        "@types/jasmine": "3.6.2",
        "@types/jasminewd2": "2.0.8",
        "@types/jquery": "3.5.4",
        "@types/mocha": "8.0.4",
